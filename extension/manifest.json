{
  "name": "Pretty Music Beta",
  "version": "1.0",
  "description": "Extension to make google music beta prettier and incorporate it better with other google products",
  "permissions": [
    "tabs", "http://*.google.com/*", "https://*.google.com/*","https://mail.google.com/*","*://mail.google.com/*"
  ],
  "content_scripts": [
    { 
      "matches": ["http://*.google.com/*","https://*.google.com/*", "https://mail.google.com/*","*://mail.google.com/*"],
	  "css":["prettymusic.css"],
	  "js":["jquery-1.6.2.min.js", "prettymusic.js"],
	  "all_frames": true
	  
    }
  ]
}